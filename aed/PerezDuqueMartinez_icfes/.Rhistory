library(zoo)
num_muestras = 1000
x <- rgamma ( num_muestras , shape = 10 , scale = 3)
x <- x + rnorm ( length (x) , mean =0 , sd = .1)
bj <- hist (x, breaks =20 , include.lowest =FALSE , right = FALSE )
bj$breaks
bj_cdf <- pgamma ( bj$breaks , shape =10 , scale =3)
pj <- rollapply ( bj_cdf , 2 , function (x) x[2] -x[1])
npj = num_muestras * pj; npj
chisq = sum((bj$counts - num_muestras*pj)^2/npj); chisq
chisquare = qchisq(0.05, df = 13, lower.tail = FALSE); chisquare
chisq.test(bj$density)
